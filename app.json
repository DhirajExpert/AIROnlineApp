{
  "expo": {
    "name": "AIROnline App",
    "slug": "AirOnline",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/app.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aironline.app",
      "newArchEnabled": true 
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app.png",
        "backgroundColor": "#d9d9d9"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "newArchEnabled": true 
    },
    "web": {
      "favicon": "./assets/app.png"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "devClient": true,
    "entryPoint": "./App.js",
    "extra": {
      "eas": {
        "projectId": "e55420d3-7072-4dd2-b2eb-6c7eb60c22cd"
      }
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app uses your camera to capture photos for your profile."
        }
      ],
      "expo-secure-store"
    ]
  }
}
